<Window x:Class="Client.Modification_Note"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="Modification_Note" Height="350" Width="525">
    <Grid>
        <TextBox Text="{Binding Titre}" HorizontalAlignment="Left" Height="23" Margin="56,39,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="125"/>
        <TextBox Text="{Binding Texte}" HorizontalAlignment="Left" Height="147" Margin="56,97,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="412"/>
        <Button  Content="Valider" Command="{Binding modif}" HorizontalAlignment="Left" Margin="362,272,0,0" VerticalAlignment="Top" Width="125"/>

    </Grid>
</Window>
